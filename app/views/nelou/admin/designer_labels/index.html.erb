<% content_for :page_title do %>
  <%= Spree.t(:designer_labels) %>
<% end %>
<table class="table" id="listing_designer_labels">
  <thead>
    <tr data-hook="admin_users_index_headers">
      <th>
        <%= Spree.t(:user) %>
      </th>
      <th>
        <%= Spree.t(:name) %>
      </th>
      <th>
        <%= Spree.t(:created_at) %>
      </th>
      <th class="actions" data-hook="admin_users_index_header_actions"></th>
    </tr>
  </thead>
  <tbody>
    <% @designer_labels.each do |designer_label| %>
      <tr data-hook="admin_designer_labels_index_rows" id="<%= spree_dom_id designer_label %>">
        <td class="user_email">
          <%= link_to designer_label.user.email, spree.edit_admin_user_url(designer_label.user) %>
        </td>
        <td>
          <%= designer_label.name %>
        </td>
        <td>
          <%= l designer_label.created_at.to_date %>
        </td>
        <td class="actions actions-2 text-right" data-hook="admin_designer_labels_index_row_actions">
          <%= link_to_edit designer_label, url: main_app.edit_admin_nelou_designer_label_url(designer_label), :no_text => true %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @designer_labels %>
